         <bottom>
            <TextField fx:id="tfInput" BorderPane.alignment="CENTER" />
         </bottom>
         <center>
            <TextArea fx:id="taOutput" editable="false" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
